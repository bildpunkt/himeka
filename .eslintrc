{
  "extends": ["prettier-standard"],
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "parser": "typescript-eslint-parser",
      "rules": {
        "no-undef": "off",
        "no-unused-vars": "off"
      }
    }
  ],
  "rules": {
    "standard/computed-property-even-spacing": "off"
  }
}
